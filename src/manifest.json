{
  "manifest_version": 1,
  "version": "1",
  "type": "APP",
  "name": {
    "ja": "鳴本カレンダー",
    "en": "kintone Event Calendar_v6.0 plug-in"
  },
  "description": {
    "ja": "FullCalendarV6と連携するプラグインです。",
